delete alignment AL_Endeis_flavivirus
delete source endeis-transcriptome
delete module nucleotideConsensusGenerator

import source sources/endeis-transcriptome/

create alignment AL_Endeis_flavivirus -r REF_APOIV
alignment AL_Endeis_flavivirus add member --whereClause "source.name = 'endeis-transcriptome'"
compute alignment AL_Endeis_flavivirus --whereClause "sequence.source = 'endeis-transcriptome'" flavivirusCompoundAligner 

create module nucleotideConsensusGenerator -t 
module nucleotideConsensusGenerator
  generate fasta nt-consensus AL_Endeis_flavivirus -a -o endeis-consensus.fna
  exit
